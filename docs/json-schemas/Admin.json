{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Admin",
  "type": "object",
  "required": ["username","password","personalInfo"],
  "properties": {
    "username": { "type": "string" },
    "password": { "type": "string" },
    "personalInfo": { "type": "object", "properties": { "firstName": {"type":"string"}, "lastName": {"type":"string"}, "email": {"type":"string","format":"email"}, "phone": {"type":"string"} } },
    "role": { "type": "string" },
    "permissions": { "type": "object" },
    "systemInfo": { "type": "object" }
  }
}
